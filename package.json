{
    "description": "",
    "name": "nui-template",
    "version": "1.0.0",
    "watch": {
        "build": {
            "patterns": [
                "static",
                "frontend"
            ],
            "extensions": "js,mjs,scss,html,css,cpp,hpp,txt",
            "delay": 1000
        }
    },
    "scripts": {
        "build": "bash scripts/build.sh",
        "watch": "npm-watch"
    },
    "devDependencies": {
        "npm-watch": "^0.11.0",
        "parcel": "^2.7.0",
        "parcel-reporter-static-files-copy": "^1.4.0",
        "rimraf": "^3.0.2"
    },
    "staticFiles": {
        "staticPath": "static/assets",
        "staticOutPath": "assets"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.2"
    }
}
